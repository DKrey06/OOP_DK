<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--BG Colours--> 
    <SolidColorBrush x:Key="AcceptDark" Color="#FF7A5C3C" />
    <SolidColorBrush x:Key="AcceptDarkOver" Color="#FFBD9975" />
    <SolidColorBrush x:Key="AcceptDarkClick" Color="#FFBDA388" />
    <SolidColorBrush x:Key="AcceptLight" Color="#FFE7C58A" />
    <SolidColorBrush x:Key="TaskChangeBG" Color="#FBEEE0" />
    <SolidColorBrush x:Key="TaskListBackground" Color="#F7E6D4" />
    <SolidColorBrush x:Key="WindowBackground" Color="#FFFFFF" />
    <SolidColorBrush x:Key="TaskStatusBackground" Color="#FBF1E7" />
    <SolidColorBrush x:Key="OnAccent" Color="#FFFFFF" />
    
    <!--Text-->
    <SolidColorBrush x:Key="DefaultText" Color="#FF474747" />
    
    <!--Importance Colours-->
    <SolidColorBrush x:Key="Importance-Low" Color="#FF0CAFF4" />
    <SolidColorBrush x:Key="Importance-Med" Color="#FF2AD985" />
    <SolidColorBrush x:Key="Importance-high" Color="#FFCA2C2C" />
    <SolidColorBrush x:Key="Importance-Crit" Color="#FFF4B30C" />

    <!--Task Progress-->
    <SolidColorBrush x:Key="Progress-InProgress" Color="#FF518657" />
    <SolidColorBrush x:Key="Progress-Complited" Color="#FF4187AC" />
    <SolidColorBrush x:Key="Progress-NotStarted" Color="#FFB1B1B1" />
    <SolidColorBrush x:Key="Progress-Late" Color="#FFB46161" />
    <SolidColorBrush x:Key="Progress-Archived" Color="#FFC8A36C" />
    <SolidColorBrush x:Key="Progress-Deleted" Color="#FF606060" />
</ResourceDictionary>